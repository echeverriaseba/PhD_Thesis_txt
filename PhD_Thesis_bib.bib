@article{fertitta-roberts2019,
  title = {Evaluating the {{GHG}} Mitigation-Potential of Alternate Wetting and Drying in Rice through Life Cycle Assessment},
  author = {{Fertitta-Roberts}, Cara and Oikawa, Patricia Y. and Darrel Jenerette, G.},
