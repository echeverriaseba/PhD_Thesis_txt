@article{meyer2021,
  title = {Limited Sink but Large Storage: {{Biomass}} Dynamics in Naturally Developing Beech ( {{{\emph{Fagus}}}}{\emph{ Sylvatica}} ) and Oak ( {{{\emph{Quercus}}}}{\emph{ Robur}} , {{{\emph{Quercus}}}}{\emph{ Petraea}} ) Forests of North-western {{Germany}}},
  shorttitle = {Limited Sink but Large Storage},
  author = {Meyer, Peter and Nagel, Rouven and Feldmann, Eike},
  year = {2021},
  month = oct,
  journal = {Journal of Ecology},
  volume = {109},
  number = {10},
  pages = {3602--3616},
  issn = {0022-0477, 1365-2745},
  doi = {10.1111/1365-2745.13740},
  urldate = {2025-05-06},
  abstract = {Abstract                                                                Currently, the dynamics underlying the storage and acquisition of biomass, and thus carbon, in naturally developing forests are under debate. A better understanding of the biomass dynamics of forests is needed to clarify the role played by naturally developing forests in the mitigation of climate change.                                                     Long-term monitoring data from unmanaged strict forest reserves (SFRs) in north-western Germany were used to analyse the biomass dynamics of pure beech, mixed beech and mixed oak forests. A complete balance of above-ground woody biomass (biomass) and growth, density-dependent and -independent mortality, as well as deadwood decay was derived. Density-independent mortality served as a proxy for disturbance severity.                                                     After a time since abandonment (TSA) of 50~years, the average biomass ranged between 334~t/ha in mixed oak and 478~t/ha in pure beech stands. The net change in biomass was positive in all forest types. Density-independent mortality and decay rates were much lower than the growth rates. Pure beech forests reached higher levels of biomass, a higher net change in biomass, and more growth than either of the mixed forest types. Biomass increased linearly with TSA in pure beech stands but followed an asymptotic course in the mixed forests. In the latter, the net change in biomass and growth were consistent with a unimodal development pattern. The development of biomass could not be explained by the ageing of the tree communities.                                                                          Synthesis                     . We hypothesized that the observed biomass dynamics are a result of the interaction between resource supply within a limited growing space and the resource-use efficiency of the tree stand in conjunction with disturbances. The still-linear increase in the biomass of pure beech forests was assumed to reflect the high resource-use efficiency of beech, especially its use of light. The above-ground capacity of naturally developing broadleaved forests to store and acquire carbon is substantial. Accordingly, allowing broadleaved forests to develop naturally can contribute substantially to carbon storage and sequestration. However, our study also suggests that the above-ground carbon sink decreases after several decades.                                                                           ,              Zusammenfassung                                                                {\"U}ber das Potenzial von W{\"a}ldern mit nat{\"u}rlicher Entwicklung als Senke und Speicher f{\"u}r Kohlenstoff wird intensiv diskutiert. Ein besseres Verst{\"a}ndnis der Biomasse- bzw. Kohlenstoffdynamik dieser W{\"a}lder wird daher dringend ben{\"o}tigt, um ihre Rolle f{\"u}r den Klimaschutz belastbar bewerten zu k{\"o}nnen.                                                     In der vorliegenden Studie wurden Zeitreihendaten aus nutzungsfreien Naturwaldreservaten in Nordwestdeutschland verwendet, um die nat{\"u}rliche Biomassedynamik von Buchen-, Buchenmisch- und Eichenmischw{\"a}ldern zu untersuchen. Im Einzelnen haben wir f{\"u}r den oberirdischen Geh{\"o}lzbestand die Biomasse, den Zuwachs, die dichteabh{\"a}ngige und die dichteunabh{\"a}ngige Mortalit{\"a}t sowie die Totholzzersetzung vollst{\"a}ndig quantifiziert. Das Ausma{\ss} der dichteunabh{\"a}ngigen Mortalit{\"a}t diente als Indikator f{\"u}r die St{\"o}rungsintensit{\"a}t.                                                     Nach einer nutzungsfreien Zeit von 50 Jahren betrug die durchschnittliche oberirdische Biomasse 334~t/ha in Eichenmischbest{\"a}nden und 478~t/ha in Buchenreinbest{\"a}nden. Die Netto{\"a}nderung der Biomasse war in allen Waldtypen positiv. Die Raten der dichteunabh{\"a}ngigen Mortalit{\"a}t und des Holzzerfalls waren deutlich niedriger als diejenigen des Zuwachses. Buchenreinbest{\"a}nde erreichten ein h{\"o}heres Biomasseniveau, h{\"o}here Netto{\"a}nderungen und Zuw{\"a}chse als die beiden Mischbestandstypen. In Buchenreinbest{\"a}nden nahm die Biomasse linear mit der nutzungsfreien Zeit zu, zeigte jedoch in Mischbest{\"a}nden einen asymptotischen Verlauf. In Letzteren entsprachen die Nettover{\"a}nderungen der Biomasse und des Zuwachses einem unimodalen Verlauf. Die Entwicklung der Biomasse konnte nicht mit der Alterung der Baumpopulationen erkl{\"a}rt werden.                                                                          Synthese                     . Wir gehen davon aus, dass die beobachtete Biomassedynamik das Ergebnis der Interaktion zwischen einem begrenzten Ressourcenangebot, der Produktivit{\"a}t der Baumbest{\"a}nde und {\"o}kologischen St{\"o}rungen ist. Die lineare Zunahme der Biomasse in Buchenreinbest{\"a}nden k{\"o}nnte durch die hohe Ressourceneffizienz der Buche, insbesondere im Hinblick auf die Lichtabsorption, bedingt sein. Unsere Ergebnisse zeigen, dass die Senken- und Speicherkapazit{\"a}t von sich nat{\"u}rlich entwickelnden Laubw{\"a}ldern bedeutend ist. Prozessschutz kann in diesen Waldtypen einen substanziellen Beitrag zum Klimaschutz leisten. Jedoch deutet unsere Analyse auch auf eine Limitierung der oberirdischen Kohlenstoffsenke nach einigen Jahrzehnten nat{\"u}rlicher Entwicklung hin.},
  langid = {english},
  file = {C:\Users\SECHEVERRIA\Zotero\storage\7SBIY9ZP\Meyer et al. - 2021 - Limited sink but large storage Biomass dynamics in naturally developing beech ( Fagus sylvatica.pdf}
}

