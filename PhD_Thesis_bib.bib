@article{altor2008,
  title = {Pulsing Hydrology, Methane Emissions and Carbon Dioxide Fluxes in Created Marshes: {{A}} 2-Year Ecosystem Study},
  shorttitle = {Pulsing Hydrology, Methane Emissions and Carbon Dioxide Fluxes in Created Marshes},
  author = {Altor, Anne E. and Mitsch, William J.},
  year = {2008},
  month = jun,
  journal = {Wetlands},
  volume = {28},
  number = {2},
  pages = {423--438},
  issn = {0277-5212, 1943-6246},
  doi = {10.1672/07-98.1},
  urldate = {2025-05-06},
  abstract = {Short-term methane and carbon dioxide flux rates were measured in two created, experimental marshes in the Midwestern U.S. over a two-year period (2004--2005) in which hydrologic conditions were manipulated to simulate flood-pulse and steady-flow conditions. Gas flux rates were measured in three distinct wetland zones: continuously inundated areas; edge zones with emergent macrophytes; and edge zones in which emergent macrophytes were removed. Methane fluxes between years were not significantly different in edge zones with and without emergent vegetation, but were twice as high in continuously inundated zones during the steady-flow year compared to the flood pulsed year. There was no apparent relationship between emergent vegetation and methane flux, as mean flux rates were not significantly different in either year in edge zones where emergent vegetation was removed, compared to edge zones containing emergent vegetation. Continuously inundated wetland zones emitted methane from summer through fall, while in edge zones methane fluxes were only substantial in spring and summer. Neither daytime rates of carbon dioxide uptake or nighttime rates of respiration were significantly different between the years for any wetland zone. When CO2 flux rates (daytime uptake plus nighttime respiration) were normalized for solar radiation and day length, solar efficiency was found to be comparable between the steady flow and pulsed years. Methane fluxes were more strongly affected than carbon dioxide fluxes by the differences in hydrology, but only in the deeper areas of the wetlands.},
  langid = {english},
  file = {C:\Users\SECHEVERRIA\Zotero\storage\TGB9U88V\Altor and Mitsch - 2008 - Pulsing hydrology, methane emissions and carbon dioxide fluxes in created marshes A 2-year ecosyste.pdf}
}

