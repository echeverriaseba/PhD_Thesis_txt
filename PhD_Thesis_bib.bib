@article{lampayan2015,
  title = {Adoption and Economics of Alternate Wetting and Drying Water Management for Irrigated Lowland Rice},
  author = {Lampayan, Rubenito M. and Rejesus, Roderick M. and Singleton, Grant R. and Bouman, Bas A.M.},
  year = {2015},
  journal = {Field Crops Research},
  volume = {170},
  pages = {95--108},
