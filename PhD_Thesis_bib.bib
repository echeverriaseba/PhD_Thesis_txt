  keywords = {Analytical framework,Boosted regression trees,Freshwater ecosystems,Generalised linear modelling,Random Forest,Water framework directive},
  file = {C:\Users\SECHEVERRIA\Zotero\storage\32ML2YM5\Feld et al. - 2016 - Analysing the impact of multiple stressors in aquatic biomonitoring data A ‘cookbook’ with applicat.pdf}
}

