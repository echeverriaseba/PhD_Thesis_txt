@article{bouman2001,
  title = {Field Water Management to Save Water and Increase Its Productivity in Irrigated Lowland Rice},
  author = {Bouman, B.A.M and Tuong, T.P},
  year = {2001},
  month = jul,
  journal = {Agricultural Water Management},
  volume = {49},
  number = {1},
  pages = {11--30},
  issn = {03783774},
  doi = {10.1016/S0378-3774(00)00128-1},
  urldate = {2025-05-06},
  abstract = {Rice production in Asia needs to increase to feed a growing population whereas water for irrigation is getting scarcer. Major challenges are to *i) save water; *ii) increase water productivity and *iii) produce more rice with less water. This study analyzes the ways in which water-saving irrigation can help to meet these challenges at the {\textregistered}eld level. The analyses are conducted using experimental data collected mostly in central{\textpm}northern India and the Philippines. Water input can be reduced by reducing ponded water depths to soil saturation or by alternate wetting/drying. Water savings under saturated soil conditions were on average 23\% * 14\%) with yield reductions of only 6\% * 6\%). Yields were reduced by 10{\textpm}40\% when soil water potentials in the root zone were allowed to reach 100 to 300 mbar. In clayey soils, intermittent drying may lead to shrinkage and cracking, thereby risking increased soil water loss, increased water requirements and decreased water productivity. Water productivity in continuous {\textasciimacron}ooded rice was typically 0.2{\textpm}0.4 g grain per kg water in India and 0.3{\textpm}1.1 g grain per kg water in the Philippines. Water-saving irrigation increases water productivity, up to a maximum of about 1.9 g grain per kg water, but decreases yield. It therefore does not produce more rice with less water on the same {\textregistered}eld. Field-level water productivity and yield can only be increased concomitantly by improving total factor productivity or by raising the yield potential. Total rice production can be increased by using water saved in one location to irrigate new land in another. If this is not done, a strategy of saving water at the {\textregistered}eld level potentially threatens total rice production at large. \# 2001 Elsevier Science B.V. All rights reserved.},
  copyright = {https://www.elsevier.com/tdm/userlicense/1.0/},
  langid = {english},
  file = {C:\Users\SECHEVERRIA\Zotero\storage\85ZI58KB\Bouman and Tuong - 2001 - Field water management to save water and increase its productivity in irrigated lowland rice.pdf}
}

