  author = {Neumann, Mathias and Moreno, Adam and Mues, Volker and H{\"a}rk{\"o}nen, Sanna and Mura, Matteo and Bouriaud, Olivier and Lang, Mait and Achten, Wouter M.J. and {Thivolle-Cazat}, Alain and Bronisz, Karol and Mergani{\v c}, J{\'a}n and Decuyper, Mathieu and Alberdi, Iciar and Astrup, Rasmus and Mohren, Frits and Hasenauer, Hubert},
