  title = {Effects of Soil Microbes on Methane Emissions from Paddy Fields under Varying Soil Oxygen Conditions},
  author = {Chunmei, Xu and Liping, Chen and Song, Chen and Guang, Chu and Xiufu, Zhang and Danying, Wang},
