@article{garbach2017,
  title = {Examining Multi-Functionality for Crop Yield and Ecosystem Services in Five Systems of Agroecological Intensification},
  author = {Garbach, Kelly and Milder, Jeffrey C. and DeClerck, Fabrice A.J. and {Montenegro de Wit}, Maywa and Driscoll, Laura and {Gemmill-Herren}, Barbara},
  year = {2017},
  journal = {International Journal of Agricultural Sustainability},
