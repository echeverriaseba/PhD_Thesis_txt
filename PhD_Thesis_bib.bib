}

@article{yan2005,
  title = {Statistical Analysis of the Major Variables Controlling Methane Emission from Rice Fields},
  author = {Yan, Xiaoyuan and Yagi, Kazuyuki and Akiyama, Hiroko and Akimoto, Hajime},
  year = {2005},
  month = jul,
  journal = {Global Change Biology},
