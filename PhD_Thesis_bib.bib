  shorttitle = {Regenerative {{Agriculture}}},
  author = {Giller, Ken E and Hijbeek, Renske and Andersson, Jens A and Sumberg, James},
  year = {2021},
  month = mar,
  journal = {Outlook on Agriculture},
  volume = {50},
