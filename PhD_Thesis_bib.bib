}

@article{belenguer-manzanedo2022,
  title = {Effect of Post-Harvest Practices on Greenhouse Gas Emissions in Rice Paddies: Flooding Regime and Straw Management},
  author = {{Belenguer-Manzanedo}, Mar{\'i}a and Alcaraz, Carles and Camacho, Antonio and Ib{\'a}{\~n}ez, Carles and {Catal{\`a}-Forner}, Mar and {Mart{\'i}nez-Eixarch}, Maite},
