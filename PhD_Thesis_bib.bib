@article{colina2021,
  title = {Trophic and Non-trophic Effects of Fish and Macroinvertebrates on Carbon Emissions},
  author = {Colina, Maite and Meerhoff, Mariana and P{\'e}rez, Germ{\'a}n and Veraart, Annelies J. and Bodelier, Paul and Van Der Horst, Antoon and Kosten, Sarian},
