}

@article{balaine2019,
  title = {Greenhouse {{Gases}} from {{Irrigated Rice Systems}} under {{Varying Severity}} of {{Alternate}}-{{Wetting}} and {{Drying Irrigation}}},
  author = {Balaine, Nimlesh and Carrijo, Daniela R. and Adviento-Borbe, M. Arlene and Linquist, Bruce},
