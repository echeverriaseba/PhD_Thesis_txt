  author = {Qian, Haoyu and Zhu, Xiangchen and Huang, Shan and Linquist, Bruce and Kuzyakov, Yakov and Wassmann, Reiner and Minamikawa, Kazunori and {Martinez-Eixarch}, Maite and Yan, Xiaoyuan and Zhou, Feng and Sander, Bjoern Ole and Zhang, Weijian and Shang, Ziyin and Zou, Jianwen and Zheng, Xunhua and Li, Ganghua and Liu, Zhenhui and Wang, Songhan and Ding, Yanfeng and {van Groenigen}, Kees Jan and Jiang, Yu},
