@article{erickson1985,
  title = {Decomposition of Logging Residues in {{Douglas-fir}}, Western Hemlock, {{Pacific}} Silver Fir, and Ponderosa Pine Ecosystems},
  author = {Erickson, Heather E. and Edmonds, R. L. and Peterson, C. E.},
