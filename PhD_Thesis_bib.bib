@article{sass1992,
  title = {Methane Emission from Rice Fields: {{The}} Effect of Floodwater Management},
  shorttitle = {Methane Emission from Rice Fields},
  author = {Sass, R. L. and Fisher, F. M. and Wang, Y. B. and Turner, F. T. and Jund, M. F.},
