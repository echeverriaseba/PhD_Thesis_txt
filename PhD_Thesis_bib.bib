@article{kahl2009,
  title = {Using Drill Resistance to Quantify the Density in Coarse Woody Debris of {{Norway}} Spruce},
  author = {Kahl, Tiemo and Wirth, Christian and Mund, Martina and B{\"o}hnisch, Gerhard and Schulze, Ernst Detlef},
  year = {2009},
  journal = {European Journal of Forest Research},
  volume = {128},
