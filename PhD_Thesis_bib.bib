@article{pandey2014,
  title = {Organic Matter and Water Management Strategies to Reduce Methane and Nitrous Oxide Emissions from Rice Paddies in {{Vietnam}}},
  author = {Pandey, Arjun and Mai, Van Trinh and Vu, Duong Quynh and Bui, Thi Phuong Loan and Mai, Thi Lan Anh and Jensen, Lars Stoumann and De Neergaard, Andreas},
