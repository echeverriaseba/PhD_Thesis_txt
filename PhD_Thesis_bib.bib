@article{martinez-eixarch2022,
  title = {Extended Methane Mitigation Capacity of a Mid-Season Drainage beyond the Rice Growing Season: A Case in {{Spain}}},
  author = {{Mart{\'i}nez-Eixarch}, Maite and {Beltr{\'a}n-Miralles}, Manuel and Gu{\'e}ry, S{\'e}bastien and Alcaraz, Carles},
  year = {2022},
  journal = {Environmental Monitoring and Assessment},
  volume = {194},
  number = {9},
