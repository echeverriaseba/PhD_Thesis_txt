@article{beets2008,
  title = {Coarse Woody Debris Decay Rates for Seven Indigenous Tree Species in the Central {{North Island}} of {{New Zealand}}},
  author = {Beets, P. N. and Hood, I. A. and Kimberley, M. O. and Oliver, G. R. and Pearce, S. H. and Gardner, J. F.},
  year = {2008},
