}

@article{keller2021,
  title = {Global Carbon Budget of Reservoirs Is Overturned by the Quantification of Drawdown Areas},
  author = {Keller, Philipp S. and Marc{\'e}, Rafael and Obrador, Biel and Koschorreck, Matthias},
