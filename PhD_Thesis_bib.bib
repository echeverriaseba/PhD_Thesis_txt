@article{nagel2008,
  title = {Gap Disturbance Regime in an Old-Growth {{{\emph{Fagus}}}} -- {{{\emph{Abies}}}} Forest in the {{Dinaric Mountains}}, {{Bosnia-Herzegovina}}},
  author = {Nagel, Thomas A. and Svoboda, Miroslav},
  year = {2008},
