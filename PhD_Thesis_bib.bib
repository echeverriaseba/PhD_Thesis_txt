  author = {Hester, Eric R. and Vaksmaa, Annika and Val{\`e}, Giampiero and Monaco, Stefano and Jetten, Mike S.M. and L{\"u}ke, Claudia},
  year = {2022},
  journal = {FEMS Microbiology Ecology},
  volume = {98},
  number = {3},
